@Article{baym97:ident:body:commun:line:life,
  Author =	 {Baym, Nancy K.},
  Title =	 {Identity, Body, and Community in On-Line Life},
  Journal =	 {Journal of Communication},
  Volume =	 {47},
  OPTnumber =	 {4},
  Pages =	 {142-148},
  OPTNote =	 {Autumn},
  Abstract =	 {A review essay on books by (1) Sherry Turkle, Life
                  on the Screen: Identity in the Age of the Internet
                  (New York: Simon and Schuster, 1995); (2) Alluquere
                  Rosanne Stone, The War of Desire and Technology at
                  the Close of the Mechanical Age (Cambridge:
                  Massachusetts Instit Technology, 1995); (3) Mike
                  Featherstone and Roger Burrows [Eds], Cyberspace,
                  Cyberbodies, Cyberpunk: Cultures of Technological
                  Embodiment (Newbury Park, ca: Sage, 1995); (4) Ralph
                  Schroeder, Possible Worlds: The Social Dynamic of
                  Virtual Reality Technology (Boulder, co: Westview,
                  4); and (5) Rob Shields (Ed), Cultures of Internet
                  (London: Sage, 1996); and (6) David Porter (Ed),
                  Internet Culture (New York: Routledge, 1997). These
                  six books explore the themes of identity, community,
                  and body in the world of computer networks. Turkle
                  considers identity from a postmodern perspective and
                  aims her work at a wide audience. Stone addresses
                  flexible identity through personal stories,
                  ethnographic parables, and an investigation of
                  institutional context. The Featherstone and Burrows
                  edition explores the topics of the machine-space
                  merger, the machine-body merger, and cyberpunk
                  fiction. Schroeder offers a detailed and informative
                  examination of the limits of current virtual reality
                  techniques. Shield's mixed collection of essays
                  digresses from its theme of "living bodies" and
                  "real histories" to get lost in theory. Porter's
                  edition provides an excellent critical and empirical
                  exploration of the Internet community. 3
                  References. M. Cella},
  Year =	 {1997}
}


