@Article{dean99:virtual:fears,
  Author =	 {Dean, Jodi},
  Title =	 {Virtual Fears},
  Journal =	 {Signs},
  Volume =	 {24},
  OPTnumber =	 {4},
  Pages =	 {1169-1078},
  OPTNote =	 {Summer},
  Abstract =	 {Examines fears related to the Internet that have
                  prompted demands for regulation on behalf of
                  "vulnerable" women and children. It is argued that
                  frightening misconceptions have prevented many women
                  from discovering resources available on the Net, and
                  have set the stage for a politics of regulation,
                  repression, and authority. Tech-anxiety, seen as a
                  product of "containment culture" that has
                  transferred fear of communism to fear of new
                  ideas/technology, mirrors current widespread
                  cultural uncertainty. Net sprawl has also created
                  demands for an authority to determine what is
                  credible in the information overload and establish a
                  foundation for truth to prevent
                  manipulation/deception that could ultimately be
                  destructive to democracy. Exploration of the link
                  between credibility and authority indicates that
                  cyberspace is a potential arena for political
                  battles over what is real involving both progressive
                  causes and those of subversive right-wing
                  groups. However, it is contended that a struggle
                  over credibility and authorization is far superior
                  to regulatory interventions that would privilege
                  some elements at the expense of others. 15
                  References. J. Lindroth},
  Year =	 {1999}
}


@article{demas97:deter,
  Author =	 {Demas, S. and Brogdon, J. L.},
  Title =	 {Determining copyright status for preservation and
                  access: Defining reasonable effort},
  Journal =	 {Library Resources \& Technical Services},
  Volume =	 {41},
  OPTnumber =	 {4},
  Pages =	 {323-334},
  OPTNote =	 {OCT LIBR RESOUR TECH SERV},
  Abstract =	 {Alternative procedures were investigated for
                  determining the copyright status of brittle
                  monographs to be scanned as part of the national
                  preservation plan for agricultural sciences
                  literature. Copyright searches were conducted both
                  in Washington in the files Of the U.S. Copyright
                  Office and at Cornell University in the printed
                  Catalog of Copyright Entries (CCE). Results were
                  compared to determine the most efficient
                  procedure. Search procedures (averaging 7 minutes
                  per title) in the CCE were 97\% in agreement overall
                  with the results obtained from considerably more
                  time consuming (13 minutes per title) searching at
                  the Copyright Office. CCE searches were 100\% in
                  agreement concerning instances of renewal of
                  copyright. This finding calls into question the
                  assumption that it is necessary to conduct such
                  searches at considerable cost in the complex files
                  of the Copyright Office. The resulting CCE search
                  procedure is suggested as a standard of reasonable
                  effort for copyright searching, which demonstrates a
                  legally responsible reasonable effort to respect the
                  rights of copyright holders while advancing
                  preservation aims and converting carefully selected
                  print materials to build the digital library.},
  Year =	 {1997}
}



