@Article{bell98:inter:trajec:techn,
  Author =	 {Bell, Daniel},
  Title =	 {The Internet and the Trajectories of Technologies},
  Journal =	 {Tocqueville Review / Revue Tocqueville},
  Volume =	 {19},
  OPTnumber =	 {2},
  Pages =	 {111-125},
  Abstract =	 {The emergence of a new communications infrastructure
                  based on technology is discussed, looking at how it
                  replaced the previous transportation-based
                  infrastructure and at the rapid rate of its
                  advance. A history of the Internet and related
                  communication technologies is provided, with
                  attention to how, although the Internet seems
                  worldwide, it is limited to countries with
                  preexisting structures like telephone systems, and
                  how the Internet is beginning to merge with tv and
                  telephone systems, despite starting out separated
                  from them. How the use of the Internet has shifted
                  from a business to a consumer context is also
                  addressed. It is held that modern technology may
                  best be understood in the form of a map and its
                  trajectories (which are presented), covering the
                  historical distinctions of technology, the sources
                  of change, the outcome for products, the character
                  of change, and the underlying trajectories of
                  technology. The Internet's relationship with the
                  "consumer ladder" is detailed, and it is argued that
                  the Internet has greatly expanded the number of
                  people who move up and down this ladder. 5
                  Grids. D. Weibel},
  Year =	 {1998}
}

