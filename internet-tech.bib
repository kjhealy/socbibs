@Article{green99:plague:panop,
  Author =	 {Green, Stephen},
  Title =	 {A Plague on the Panopticon: Surveillance and Power
                  in the Global Information Economy},
  Journal =	 {Information, Communication and Society},
  Volume =	 {2},
  Number =	 {1},
  Pages =	 {26-44},
  OPTNote =	 {Spring},
  Abstract =	 {Jeremy Bentham's (1791) panopticon has, courtesy of
                  Michel Foucault (1975), become a dominant metaphor
                  in surveillance studies. This central eye, aimed at
                  the moral reform of institutionalized bodies and
                  operating through a de facto closure of individual
                  agency, tends to limit the conception of the complex
                  ways surveillance serves and disrupts power. Via
                  analysis of commercial and workplace surveillance,
                  it is shown here that power is contested, and agency
                  is never completely closed down. To comprehend this
                  negotiated relationship, an alternative framework -
                  plague management - is presented. This envisions
                  surveillance in a social setting operated by
                  multiple agents who themselves are open to the gaze
                  of those surveyed. The aim of this project is to
                  categorize rather than to reform and cure, and,
                  moreover, space is available for resistance. On the
                  global scale, new technologies have opened space for
                  informal, local surveillances to be effectively
                  linked to media and Internet audiences. These
                  surveillance networks, informal alliances of a
                  variety of actors, have the potential to radically
                  alter orthodox relations of power by evading the
                  information controls of the state and connecting a
                  local gaze with the global community. It is
                  concluded that, although surveillance studies has
                  rightly been concerned with the control capacities
                  of information technology, a balance must be
                  actively sought with the facilitative, enabling, and
                  even democratic impulses nascent in the new
                  technology.},
  Year =	 {1999}
}

