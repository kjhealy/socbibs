@Article{	  jaccard97:secur,
  author	= {Jaccard, M. A.},
  title		= {Securing copyright in transnational cyberspace: The case
		  for contracting with potential infringers},
  journal	= {Columbia Journal of Transnational Law},
  volume	= {35},
  number	= {3},
  pages		= {619--662},
  optnote	= {COLUMBIA J TRANSNAT LAW},
  abstract	= {This article analyzes the current state of copyright
		  protection for electronically disseminated works on an open
		  network such as the Internet. The author points out that
		  worldwide networks are potentially very powerful and
		  inexpensive publishing tools, though many copyright holders
		  are still afraid to disseminate their works electronically
		  for fear of massive infringement taking place outside the
		  jurisdiction of U.S. courts. The author argues, however,
		  that it is not accurate to describe cyberspace as an
		  entirely ``lawless land'' where copyright infringers can
		  act anonymously. Though he agrees with many commentators
		  that ideally national copyright laws will need to be
		  harmonized, e.g., through the multilateral process already
		  initiated under the authority of the World Intellectual
		  Property Organization, the author argues that even today
		  contract law provides copyright owners with some viable
		  ways to protect their intellectual property in cyberspace.
		  He proposes a number of steps that they can take so as to
		  create binding contracts with potential infringers willing
		  to acquire the copyrighted works on-line throughout the
		  world. Such an agreement would provide sufficient
		  protection and would constitute the foundation for
		  acquiring royalties. While discussing transnational aspects
		  of cross-border dissemination of electronic works, the
		  author analyzes also the drafting process of the new art.
		  2B U.C.C. (licenses) and the limits of state contract law
		  in the light of federal copyright protection in the U.S.
		  Finally, he points out that no legal protection, by
		  statute, multilateral treaty or contract, is secure unless
		  some kind of cryptography is used for the transmitted
		  works.},
  year		= {1997}
}

