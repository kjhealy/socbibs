@Article{	  wellman99:livin:networ:offlin,
  author	= {Wellman, Barry and Hampton, Keith},
  title		= {Living Networked On and Offline},
  journal	= {Contemporary Sociology},
  volume	= {28},
  number	= {6},
  pages		= {648--654},
  optnote	= {Nov},
  abstract	= {Uses research done on Netville, a 'wired' community, to
		  discuss a paradigm shift from social groups to network
		  societies as driven by computer-Internet technologies.
		  Implications in computer networks for social relationships
		  are discussed, indicating that online connections mix with
		  offline face-to-face interactions to strengthen ties at
		  both the individual and community levels. Netville social
		  relationships fostered individual-community inclusion,
		  rather than being a cause of increased fragmentation and
		  alienation as feared by dystopians. It is concluded that
		  computer networks constitute a positive social phenomenon
		  that will enhance social relationships and help to build
		  stronger communities. 46 References. T. Noland},
  year		= {1999}
}

