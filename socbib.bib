@Book{		  ludlow01:_crypt_anarc_cyber_pirat_utopias,
  editor	= {Peter Ludlow},
  title		= {Crypto Anarchy, Cyberstates and Pirate Utopias},
  publisher	= {MIT Press},
  year		= {2001},
  optkey	= {},
