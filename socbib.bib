@Article{	  selwyn99:school:infor:societ,
  author	= {Selwyn, Neil},
  title		= {Schooling the Information Society? The Place of the
		  Information Superhighway in Education},
  journal	= {Information, Communication and Society},
  volume	= {2},
  number	= {2},
  pages		= {156--173},
  optnote	= {Summer},
  abstract	= {Despite a history of educational resistance to technology,
		  the Internet and, in turn, the information superhighway
		  have been popularly heralded as having the potential to
		  transform schools the world over, but little critical
		  examination of the information superhighway's role in
		  education has taken place. Here, popular conceptions of the
		  educational superhighway are contrasted with the likely
		  social and cultural implications of the 'wired' school.
		  Examined first are three central claims made about the
		  information superhighway in education: (1) the unbridled
		  access to information it will afford teachers and learners;
		  (2) the potential for interactive communication with other
		  individuals; and (1990) The equality it will imbue. These
		  popular discourses are then contrasted with two fundamental
		  characteristics of the information superhighway often
		  overlooked by its advocates: the different quality of
		  learning experienced online and the educational
		  implications of the inherent economic nature of the
		  emerging information superhighway. Suggested in conclusion
		  is an alternative approach to examining the implementation
		  of the information superhighway in an educational context.
		  51 References. Adapted from the source document},
  year		= {1999}
}

